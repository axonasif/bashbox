NAME="BashBox"
CODENAME="bashbox"
AUTHORS=("AXON <axonasif@gmail.com>")
VERSION="0.3.9"
DEPENDENCIES=(
	https://github.com/bashbox/std.git
	argbash::0.1.0
)
